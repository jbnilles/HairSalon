@{
  Layout = "_Layout";
}
@model HairSalon.Models.Cuisine


<div class="container">
  <div class="jumbotron">
    <h4>Edit a Stylist!</h4>
  </div>
    
    @using (Html.BeginForm())
{
    @Html.HiddenFor(model => model.StylistId)
    @Html.LabelFor(model => model.StylistName)
    @Html.EditorFor(model => model.StylistName)
    <input type="submit" value="Edit" />
}
    <p>@Html.ActionLink("Show all Stylist", "Index")</p>
</div>
@Html.Partial("Header")
@Html.Partial("Footer")