@{
  Layout = "_Layout";
}
@model HairSalon.Models.Stylist
<div class="container-fluid">
  <div class="jumbotron">
    Create a stylist account
  </div>
  @using(Html.BeginForm())
  {
    @Html.LabelFor(model => model.StylistName)
    @Html.TextBoxFor(model => model.StylistName)
    <input type="submit" value="Create Stylist">
  }
  <p>@Html.ActionLink("Show all Stylist", "Index")</p>
</div>
@Html.Partial("Header")
@Html.Partial("Footer")